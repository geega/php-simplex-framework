{
    "name": "geega/php-simplex-framework",
    "type": "library",
    "description": "Simple php framework",
    "minimum-stability" : "dev",
    "require": {
        "php" : "~5.6|~7.0",
        "geega/php-request": "*",
        "geega/php-response": ">0.0.1",
        "geega/php-simplex-native-template" : "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Geega\\Simplex\\": "src",
            "app\\" : "tmp\\app"
        }
    }
}
